{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "convex/schema": ["./node_modules/convex/dist/cjs-types/server/schema"],
      "convex/values": ["./node_modules/convex/dist/cjs-types/server/values"],
      "convex/server": ["./node_modules/convex/dist/cjs-types/server/index"],
      "convex/react": ["./node_modules/convex/dist/cjs-types/react/index"],
      "convex/browser": ["./node_modules/convex/dist/cjs-types/browser/index"]
    },
    "strictNullChecks": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
